<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.zhiyou100.video.model.User" table="user">
    <id name="id">
    <!-- 主键生成策略 -->
    <generator class="native"></generator>
    </id>
    <property name="nick_name"></property>
    <property name="sex"></property>
    <property name="birthday"></property>
    <property name="email"></property>
    <property name="province"></property>
    <property name="head_url"></property>
    <property name="city"></property>
    <property name="password"></property>
    <property name="insert_time"></property>
    <property name="update_time"></property>
    <property name="captcha"></property>
    
    </class>
    </hibernate-mapping>